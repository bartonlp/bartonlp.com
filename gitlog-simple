  +// Strip Comments
  +
  +if(!function_exists('stripComments')) {
  +  function stripComments($x) {
  +    $pat = '~".*?"(*SKIP)(*FAIL)|(?://[^\n]*)|(?:#[^\n]*)|(?:/\*.*?\*/)~s';
  +    return preg_replace($pat, "", $x);
  +  }
  +}

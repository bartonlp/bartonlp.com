  +// Put a line with escaping
  +
  +if(!function_exists('put')) {
  +  function put($msg) {
  +    $msg = escapeltgt($msg);
  +    echo "<pre>$msg</pre>\n";
  +  }
  +}
  +
  +// Put a line with no escaping
  +
  +if(!function_exists('putNoEscape')) {
  +  function putNoEscape($msg) {

| +function callback($class) {
| +  switch($class) {
| +    case "SimpleSiteClass":
| +      require(__DIR__ . "/../../includes/$class.php");
| +      break;
| +    default:
| +      $class = preg_replace("~Simple~", "", $class);
| +      require(__DIR__ . "/../../includes/database-engines/$class.class.php");
| +      break;
| +  }
| +}

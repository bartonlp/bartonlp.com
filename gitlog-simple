| -<span id="cb4-4"><a href="#cb4-4" aria-hidden="true"></a>   <span class="ex">*</span>   array(<span class="st">&quot;&lt;td&gt;&quot;</span>, <span class="st">&quot;&lt;/td&gt;&quot;</span>);</span>

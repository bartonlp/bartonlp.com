| -<span id="cb4-22"><a href="#cb4-22" aria-hidden="true"></a>  <span class="ex">public</span> function makeresultrows(<span class="va">$query</span>, <span class="va">$rowdesc</span>, array <span class="va">$extra</span>=array()) <span class="dt">{...}</span></span></code></pre></div>
| +<div class="sourceCode" id="cb3"><pre
